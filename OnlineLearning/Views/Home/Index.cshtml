        $('.tab-content:first').show();
        $('#tab6').show();


        $('#tabs-nav li').click(function(){
            $('#tabs-nav li').removeClass('active-bl');
            $(this).addClass('active-bl');
            $('.tab-content').hide();
            var activeTab = $(this).find('a').attr('href');
            $(activeTab).fadeIn();
            return false;
        });

