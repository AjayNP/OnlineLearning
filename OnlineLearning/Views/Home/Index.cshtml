    <script>
        jQuery(document).ready(function ($) {
            $('.tab-content video').trigger('pause');
            $('.tab-content:first-child video').trigger('play');
            $(".pur-c").click(function(){

                $(".pop-up").css("display","block");
            });
            $("form i").click(function(){
                $(".pop-up").css("display","none");
            });
            $(".msg-bk").hide();
            $(".show-msg").click(function(){
                $(".msg-bk").show();
                $("form").hide();
            });
            $(".close-btn").click(function(){
                $(".pop-up").hide();
            });
        });
    </script>
    <script type="text/javascript">
        $('#tabs-nav li:first-child').addClass('active-bl');
